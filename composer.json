{
    "name": "shone/scanner",
    "description": "Software Version Scanner",
    "keywords": ["version", "scanner", "security", "vulnerability"],
    "homepage": "https://www.shone.co.za/",
    "type": "tool",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Shone",
            "email": "thomas@shone.co.za",
            "homepage": "https://www.shone.co.za"
        }
    ],
    "support": {
        "issues": "https://github.com/xsist10/shone-scanner/issues"
    },
    "require": {
        "php": ">=5.3.2",
        "symfony/console": "~2.3@dev",
        "symfony/process": "~2.1@dev",
        "symfony/finder": "2.5.*@dev",
        "league/flysystem": "0.2.*"
    },
    "require-dev": {
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-4": { "Shone\\Scanner\\": "src/" }
    },
    "bin": ["bin/shone"],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
